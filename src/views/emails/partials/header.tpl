<body width="100%" bgcolor="#F5F5F6" style="margin: 0; mso-line-height-rule: exactly;">
    <center style="width: 100%; background: #F5F5F6; text-align: left;">

        <div style="max-width: 600px; margin: auto;{{{ if rtl }}} text-align: right; direction: rtl;{{{ end }}}" class="email-container">
            <!--[if mso]>
            <table role="presentation" cellspacing="0" cellpadding="0" border="0" width="600" align="center">
            <tr>
            <td>
            <![endif]-->

            <!-- Email Header : BEGIN -->
            <table role="presentation" cellspacing="0" cellpadding="0" border="0" align="center" width="100%" style="max-width: 600px;">
                <tr>
                    <td style="padding: 32px 20px; text-align: center; background: #F5F5F6;">
                        {{{ if logo.src }}}
                        <img src="{logo.src}" height="auto" width="{logo.width}" alt="{site_title}" border="0" style="height: auto; width: {logo.width}px; max-width: 180px; background: transparent; font-family: -apple-system,BlinkMacSystemFont,Segoe UI,Helvetica,Arial,sans-serif; font-size: 15px; line-height: 20px; color: #333333;">
                        {{{ else }}}
                        <div style="font-family: -apple-system,BlinkMacSystemFont,Segoe UI,Helvetica,Arial,sans-serif; font-size: 20px; font-weight: 700; color: #0029FF;">{site_title}</div>
                        {{{ end }}}
                    </td>
                </tr>
            </table>
            <!-- Email Header : END -->
