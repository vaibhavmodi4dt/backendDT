    <!-- CSS Reset -->
    <style>
        html,
        body {
            margin: 0 auto !important;
            padding: 0 !important;
            height: 100% !important;
            width: 100% !important;
        }

        * {
            -ms-text-size-adjust: 100%;
            -webkit-text-size-adjust: 100%;
        }

        div[style*="margin: 16px 0"] {
            margin:0 !important;
        }

        table,
        td {
            mso-table-lspace: 0pt !important;
            mso-table-rspace: 0pt !important;
        }

        table {
            border-spacing: 0 !important;
            border-collapse: collapse !important;
            table-layout: fixed !important;
            margin: 0 auto !important;
        }
        table table table {
            table-layout: auto;
        }

        img {
            -ms-interpolation-mode:bicubic;
        }

        *[x-apple-data-detectors],
        .x-gmail-data-detectors,
        .x-gmail-data-detectors *,
        .aBn {
            border-bottom: 0 !important;
            cursor: default !important;
            color: inherit !important;
            text-decoration: none !important;
            font-size: inherit !important;
            font-family: inherit !important;
            font-weight: inherit !important;
            line-height: inherit !important;
        }

        .a6S {
            display: none !important;
            opacity: 0.01 !important;
        }

        img.g-img + div {
            display:none !important;
        }

        .button-link {
            text-decoration: none !important;
        }

        @media only screen and (min-device-width: 375px) and (max-device-width: 413px) {
            .email-container {
                min-width: 375px !important;
            }
        }
    </style>

    <!-- Design System Styles -->
    <style>
        :root {
            /* Design System Colors */
            --color-primary: #0029FF;
            --color-primary-content: #FFFFFF;
            --color-base-100: #FEFEFF;
            --color-base-200: #F5F5F6;
            --color-base-300: #E5E5E6;
            --color-base-content: #000000;
            --color-neutral: #333333;
            --color-neutral-content: #F5F5F5;
            --color-accent: #000B47;
            
            /* Border Radius */
            --radius-sm: 0.25rem;
            --radius-md: 0.375rem;
            --radius-lg: 0.5rem;
            --radius-xl: 0.75rem;
            
            /* Spacing */
            --spacing-xs: 0.25rem;
            --spacing-sm: 0.5rem;
            --spacing-md: 0.75rem;
            --spacing-lg: 1rem;
            --spacing-xl: 1.5rem;
            --spacing-2xl: 2rem;
            --spacing-3xl: 2.5rem;
            --spacing-4xl: 3rem;
        }

        /* Button Styles */
        .button-primary {
            background: #0029FF !important;
            border: 2px solid #0029FF !important;
            border-radius: 8px !important;
            color: #FFFFFF !important;
            font-weight: 600 !important;
            transition: all 200ms ease !important;
        }

        .button-primary:hover {
            background: #001FCC !important;
            border-color: #001FCC !important;
        }

        .button-outline {
            background: transparent !important;
            border: 2px solid #0029FF !important;
            border-radius: 8px !important;
            color: #0029FF !important;
            font-weight: 600 !important;
            transition: all 200ms ease !important;
        }

        .button-outline:hover {
            background: #F0F2FF !important;
        }

        /* Typography */
        .heading-1 {
            font-size: 24px !important;
            line-height: 32px !important;
            font-weight: 700 !important;
            color: #000000 !important;
            margin: 0 !important;
        }

        .heading-2 {
            font-size: 20px !important;
            line-height: 28px !important;
            font-weight: 600 !important;
            color: #000000 !important;
            margin: 0 !important;
        }

        .heading-3 {
            font-size: 16px !important;
            line-height: 24px !important;
            font-weight: 600 !important;
            color: #333333 !important;
            margin: 0 !important;
        }

        .body-text {
            font-size: 15px !important;
            line-height: 24px !important;
            color: #333333 !important;
            margin: 0 !important;
        }

        .text-muted {
            color: #666666 !important;
        }

        /* Card Styles */
        .card {
            background: #FFFFFF !important;
            border: 1px solid #E5E5E6 !important;
            border-radius: 8px !important;
        }

        .card-bordered {
            border: 2px solid #E5E5E6 !important;
        }

        /* Notification Body Images */
        .notification-body img {
            max-width: 100%;
            height: auto;
            border-radius: 6px;
        }

        /* Media Queries */
        @media screen and (max-width: 600px) {
            .email-container p {
                font-size: 15px !important;
                line-height: 24px !important;
            }

            .heading-1 {
                font-size: 22px !important;
                line-height: 30px !important;
            }

            .heading-2 {
                font-size: 18px !important;
                line-height: 26px !important;
            }
        }
    </style>

